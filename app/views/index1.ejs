        }

        /*Fim dos Botões*/

        /*Ródapé*/

        footer {
        margin-top: 500;
        width: 1899px;
        height: 185px;
        color:  #000000;
        background-color: #eaeaea;
        border: 2px solid #BDBDBD;
        }
        .img{
        height: 30px;
        float: right;
        padding: 10px;
        }

        .copyright{
        margin-top: 50px;
        background-color: #BDBDBD;
        width: 1899px;
        height: 50px;
        text-align: center;
        }

        .logodois{
        padding: 10px;
        height: 65px;
        }

        .rodape{
        width: 50%;
        }

        .social{
        float: right;
        width: 50%;
        margin-top: -80px ;
        }

        #subTela1{
        border: solid 1px #000000;
        background-color: #e6e6e6;
	    width: 100px;
        height: 100px;
	    position: absolute;
	    text-align: center;
        transform: translate(-50%, -50%);
        margin-left: 88%;
        margin-top: 70px;
	    font-size: 13px;
        }

        #sair {
        font-size: 19px;
        text-decoration: none;
        font-weight: bold;
        color: #000000;
        }

        /*Ródapé*/
    </style>

    <body>

        <!--Cabeçalho-->
        
        <header class="menuum">
            <main>
                <div class="cabecalho">
                    <div class="logo">
                        <img src="Logo.png">
                    </div>
                    
                    <div class="linha">
                        <p>Bem vindo Usuário!</p>
                    </div>

                    <div id="subTela1" style="display: none;"><br>


                        <a href="http://10.144.166.92:3000" id="sair">Logout</a><br>

                        <h2 id="close">Voltar</h2>
                    </div>

                    <div class="login">
                        <div class="casa">
                            <a id="Relat1">
                                <img src="Login.png">
                            </a>
                        </div>
                
                
                        <script>
                            //Evento para abrir a SubTela
                            document.getElementById("Relat1").addEventListener("click", function () {
                                abrirSubTela1();
                            });
                    
                            //Evento para fechar a SubTela
                            document.getElementById("close").addEventListener("click", function () {
                                fecharSubTela1();
                            });
                    
                            function abrirSubTela1() {
                                document.getElementById("subTela1").style.display = "block";
                            }
                    
                            function fecharSubTela1() {
                                document.getElementById("subTela1").style.display = "none";
                            }
                        </script>
                
                    </div>
                    </div>
                </div>
            </main>
        </header>

        <header class="menudois">
            <main>
                <div class="menu">
                    <div class="itens">
                        <ul>
                            <li>
                                <a href="http://10.144.166.92:3000/sobrenoslogado">Sobre nós</a>
                            </li>
      
                            <li>
                                <a href="http://10.144.166.92:3000/produtos">Produtos</a>
                            </li>
      
                        </ul>
                    </div>
                    <div class="busca">
                        <div class="buscaram">
                            <h10 class="h10">Ops... Barra de pesquisa não está funcionando no momento</h1>
                            <img src="Lupa.png" class="lupa">
                        </div>
                    </div>
                </div>                
            </main>
        </header>

        <header class="help">
            <main>
                <div class="endereco">
                    <p class="informacoes">
                        Fale conosco:
                    </p>
        
                    <img src="Telefone.png" class="tell">
        
                    <p class="informacoes">
                        (19) 99999-9999 ou (19) 3466-9999
                    </p>
        
                    <img src="Localização.png" class="loc">
        
                    <p class="informacoes">
                        Rua: Ana Clara Fernades de Oliveira, Bairro das Anas, 30
                    </p>
                </div>
            </main>
        </header>

        <!--Fim do Cabeçalho-->

        <!--Carrossel-->
    
        <div class="slider"> 

            <div class="slides">

                <input type="radio" name="radio-btn" id="radio1">
                <input type="radio" name="radio-btn" id="radio2">
                <input type="radio" name="radio-btn" id="radio3">
                <input type="radio" name="radio-btn" id="radio4">

                <div class="slide first">
                    <img src="Welcome.png" alt="Imagem 1">
                </div>
                <div class="slide">
                    <img src="Cidade.png" alt="Imagem 2">
                </div>
                <div class="slide">
                    <img src="Medicos.png" alt="Imagem 3">
                </div>
                <div class="slide">
                    <img src="Medicamentos.png" alt="Imagem 4">
                </div>

                <div class="navigation-auto">
                    <div class="auto-btn1"></div>
                    <div class="auto-btn2"></div>
                    <div class="auto-btn3"></div>
                    <div class="auto-btn4"></div>
                </div>

            </div>

            <div class="manual-navigation">
                <label for="radio1" class="manual-btn"></label>
                <label for="radio2" class="manual-btn"></label>
                <label for="radio3" class="manual-btn"></label>
                <label for="radio4" class="manual-btn"></label>
            </div>

        </div>

        <script>
            //Carrossel//

            let count = 1;
            document.getElementById("radio1").checked = true;

            setInterval( function(){
                nextImage();
            }, 5000)

            function nextImage(){
                count++;
                if(count>4){
                    count = 1;
                }

                document.getElementById("radio"+count).checked = true;
            } 
        </script>

        <!--Fim do Carrossel-->

        <!--Botões-->

        <main>
            <div class="oque">
                <p>O que você deseja?</p>
            </div>

            <div class="botao">
                <a href="http://10.144.166.92:3000/planos">
                    <button class="planos" ><img src="Planos.png"><br>Planos de saúde</button>
                </a>
                <a href="http://10.144.166.92:3000/consultas">
                    <button class="agendar"><img src="Consultas.png"><br>Agendar consultas</button>
                </a>
                <a href="http://10.144.166.92:3000/doacoes">
                    <button class="doacoes"><img src="Carteira.png"><br>Doações</button>
                </a>
                <a href="http://10.144.166.92:3000/testes">
                    <button class="verificar"><img src="Testes.png"><br>Verificar testes</button>
                </a>
            </div>
        </main>

        <!--Fim dos Botões-->

        <!--Ródapé-->

        <footer>
            <main>
                <div id="footer_content">
                    <div class=rodape>
                        <img src="Logo.png" class="logodois">
                    </div>

                    <div class="social">
                        <a href="http://10.144.166.92:3000/sociaislogado" class="footer-link" id="instagram">
                            <img src="Instagram.png" class="img">
                        </a>
                        <a href="http://10.144.166.92:3000/sociaislogado" class="footer-link" id="facebook">
                            <img src="Facebook.png" class="img">
                        </a>
                        <a href="http://10.144.166.92:3000/sociaislogado" class="footer-link" id="twitter">
                            <img src="Twitter.png" class="img">
                        </a>
                    </div>
                </div>

            </main>
            <div class="copyright">
                <p>
