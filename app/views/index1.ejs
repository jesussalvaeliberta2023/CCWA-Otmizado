        .tell {
        margin-top: 8px;
        margin-left: -6px;
        position: absolute;
        
        }

        .loc{
        margin-top: 6px;
        margin-left: -6px;
        position: absolute;
        
        }

        /*Fim do Cabeçalho*/

        /* Carrossel de imagens */

        .slider{
        margin: 0 auto;
        width: 980px;
        height: 350px;
        overflow: hidden;
        }

        .slides{
        width: 400%;
        height: 350px;
        display: flex;
        }

        .slides input{
        display: none;
        }

        .slide{
        width: 25%;
        position: relative;
        transition: 1s;
        }

        .slide img{
        width: 980px;
        }

        .manual-navigation{
        position: absolute;
        width: 980px;
        margin-top: -22px;
        display: flex;
        justify-content: center;
        }

        .manual-btn{
        border: 2px solid #000000;
        padding: 5px;
        border-radius: 10px;
        cursor: pointer;
        transition: 0.5s;
        }

        .manual-btn:not(:last-child){
        margin-right: 5px;
        }

        .manual-btn:hover{
        background-color: #242323;
        }

        #radio1:checked ~ .first{
        margin-left: 0;
        }

        #radio2:checked ~ .first{
        margin-left: -25%;
        }

        #radio3:checked ~ .first{
        margin-left: -50%;
        }

        #radio4:checked ~ .first{
        margin-left: -75%;
        }

        .navigation-auto div{
        border: 2px solid #00aeff;
        padding: 5px;
        border-radius: 10px;
        cursor: pointer;
        transition: 1s;
        }

        .navigation-auto{
        position: absolute;
        width: 980px;
        margin-top: 328px;
        display: flex;
        justify-content: center;
        }

        .navigation-auto div:not(:last-child){
        margin-right: 5px;
        }


        #radio1:checked ~ .navigation-auto .auto-btn1{
        background-color: #242323;
        }


        #radio2:checked ~ .navigation-auto .auto-btn2{
        background-color: #242323;
        }


        #radio3:checked ~ .navigation-auto .auto-btn3{
        background-color: #242323;
        }


        #radio4:checked ~ .navigation-auto .auto-btn4{
        background-color: #242323;
        }

        /* Fim do Carrossel de imagens*/

        /*Pergunta*/

        .oque{
        margin-top: 75PX;
        text-align: center;
        font-family: Gruppo;
        color: #a8a8a8;
        font-size: 35px;
        }

        /*Fim da Pergunta*/

        /*Botões*/

        .botao{
        margin-top: 20px;
        }

        .planos{
        margin: 0 auto;
        position: absolute;
        width: 257px;
        height: 370px;
        border: 0.5px solid #9cdfb7;
        border-radius: 10px;
        background-color:  #C4FBDA;
        text-align: center;
        font-size: 23px;
        }

        .planos:hover{
